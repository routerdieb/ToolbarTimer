{
  "name": "TopTimer",
  "version": "0.0.5",
  "manifest_version": 3,
  "description": "TopTimer is a
   timer for sites. Especially for evernote, notion, kanban boards.
  ",
  "options_ui": {
    "page": "html/options.html",
    "open_in_tab": true
  },
  "content_scripts": [
    {
      "matches": ["https://*/*","http://*/*"],
      "css": ["/css/style.css","css/errorDiv.css"],
      "js": ["js/jquery-3.6.0.slim.min.js","js/utils.js","js/loadStore.js","js/options.js", "js/calendar.js","js/script.js"]
    }
  ],
  "web_accessible_resources": [{
    "resources":["media/engine-start.mp3","media/ring.mp3","media/calendar-icon.png",
		"media/gear-icon.png","media/engine-start.mp3","media/ring.mp3","css/styles.css"],
	"matches": ["<all_urls>"],
       "extension_ids": []
  }],
  "permissions": ["storage"]
}
